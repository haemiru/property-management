{
  "expo": {
    "name": "μ¤‘κ°λ…ΈνΈ",
    "slug": "junggae-note",
    "version": "1.0.6",
    "scheme": "junggae-note",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#4f46e5"
